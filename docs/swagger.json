                    "type": "string",
                    "example": "dat1@gmail.com"
                },
                "password": {
                    "description": "The password for user",
                    "type": "string",
                    "example": "123456789"
                }
            }
        },
        "RegisterSwagger": {
            "type": "object",
            "required": [
                "email",
                "name",
                "password"
            ],
            "properties": {
                "email": {
                    "description": "The email for user",
                    "type": "string",
                    "example": "dat1@gmail.com"
                },
                "name": {
                    "description": "The name for user",
                    "type": "string",
                    "example": "dat"
                },
                "password": {
                    "description": "The password for user",
                    "type": "string",
                    "example": "123456789"
                }
            }
